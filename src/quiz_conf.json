{
	#######
	# DATASET PATHS
	#######
	"image_dataset": "../data/caltech101/101_ObjectCategories/Motorbikes",
	"image_annotations": "../data/caltech101/Annotations/Motorbikes_16",
	"image_distractions": "../data/SceneClass13",

	#######
	# FEATURE EXTRACTION
	#######
	"features_path": "../output/cars/car_features.hdf5",
	"percent_gt_images": 0.75,
	"offset": 5,
	"use_flip": true,
	"num_distraction_images": 1000,
	"num_distractions_per_image": 20,

	#######
	# HISTOGRAM OF ORIENTED GRADIENTS DESCRIPTOR
	#######
	"orientations": 9,
	"pixels_per_cell": [4, 4],
	"cells_per_block": [2, 2],
	"normalize": true,

	#######
	# OBJECT DETECTOR
	#######
	"window_step": 4,
	"overlap_thresh": 0.3,
	"pyramid_scale": 1.5,
	"window_dim": [96, 32],
	"min_probability": 0.7
}

